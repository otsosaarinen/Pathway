<Page x:Class="Pathway.NewFileTransferPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pathway"
      mc:Ignorable="d" 
      d:DesignHeight="100" d:DesignWidth="300"
      Title="NewFileTransferPage">

	<Grid>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
			<TextBlock Text="New File Transfer Setup" FontSize="24" Margin="0,0,0,20"/>
			<Button Content="Back to Home" 
                    Click="BackToHome_Click" 
                    Width="200" Height="40"/>
		</StackPanel>
	</Grid>
	
</Page>
